{
  "compilerOptions": {
    "module": "commonjs",
    "esModuleInterop": true,
    "target": "es2016",
    "moduleResolution": "node",
    "sourceMap": true,
    "outDir": "dist",
    "strict": true,
    "noImplicitAny": true,
    "noUncheckedIndexedAccess": true,
    "strictPropertyInitialization": true,
    "strictNullChecks": true,
    "noUnusedParameters": true,
    "noUnusedLocals": true
  }
}
